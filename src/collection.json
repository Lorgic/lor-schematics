{

  "$schema": "./node_modules/@angular-devkit/schematics/collection-schema.json",

 
  "schematics": {
    "nn-ngrx-state": {
      "description": "Setup state with NN App project structure. (Uses ngrx/schematics)",
      "factory": "./nn-state",
      "schema": "./nn-state/schema.json"

    },
    "nn-ngrx-action": {
      "description": "Setup actions",
      "factory": "./nn-action",
      "schema": "./nn-action/schema.json"

    },
    "nn-ngrx-effect": {
      "description": "Setup effects",
      "factory": "./nn-effect",
      "schema": "./nn-effect/schema.json"
    },
    "nn-ngrx-reducer": {
      "description": "Setup reducer",
      "factory": "./nn-reducer",
      "schema": "./nn-reducer/schema.json"
    },
    "nn-ngrx-selector": {
      "description": "Setup selectors",
      "factory": "./nn-selector",
      "schema": "./nn-selector/schema.json"
    }
  }
}
