{

  "$schema": "./node_modules/@angular-devkit/schematics/collection-schema.json",

 
  "schematics": {
    "lor-ngrx-state": {
      "description": "Setup state with Lorgic App project structure. (Uses ngrx/schematics)",
      "factory": "./lor-state",
      "schema": "./lor-state/schema.json"

    },
    "lor-ngrx-action": {
      "description": "Setup actions",
      "factory": "./lor-action",
      "schema": "./lor-action/schema.json"

    },
    "lor-ngrx-effect": {
      "description": "Setup effects",
      "factory": "./lor-effect",
      "schema": "./lor-effect/schema.json"
    },
    "lor-ngrx-reducer": {
      "description": "Setup reducer",
      "factory": "./lor-reducer",
      "schema": "./lor-reducer/schema.json"
    },
    "lor-ngrx-selector": {
      "description": "Setup selectors",
      "factory": "./lor-selector",
      "schema": "./lor-selector/schema.json"
    }
  }
}
